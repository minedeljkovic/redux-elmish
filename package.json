    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.0.1",
    "estraverse-fb": "^1.3.1",
    "isparta": "^4.0.0",
    "mocha": "^2.4.5",
    "react": "^15.0.2",
    "redux": "^3.5.2",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3"
  },
  "dependencies": {
    "recompose": "^0.18.0",
    "redux-loop": "^2.1.1"
